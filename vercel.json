{
  "version": 2,
  "functions": {
    "public/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/(css|js|images)/(.+)",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|webp))",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/public/index.php"
    }
  ]
}
